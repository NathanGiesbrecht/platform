{
    "name": "superv/platform",
    "description": "SuperV Platform Package",
    "autoload": {
        "psr-4": {
            "SuperV\\Platform\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
            "Tests\\Platform\\": "tests/Platform"
        }
    },
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "superv/assetic": "dev-master",
        "superv/guard-module": "^0.3.0",
        "superv/nucleo-module": "^0.3.0",
        "superv/composer-plugin": "~0.1.0",
        "laravel/framework": "5.7.*",
        "rcrowe/twigbridge": "^0.9.6",
        "barryvdh/laravel-cors": "^0.11.2",
        "tymon/jwt-auth": "dev-develop",
        "nicmart/string-template": "~0.1",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "SuperV\\Platform\\PlatformServiceProvider"
            ]
        }
    }
    
}
