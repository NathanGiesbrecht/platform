{
    "name": "superv/platform",
    "description": "SuperV Platform Package",
    "autoload": {
        "psr-4": {
            "SuperV\\Platform\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
            "Tests\\Platform\\": "tests/Platform"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "fideloper/proxy": "~4.0",
        "superv/assetic": "dev-master",
        "laravel/framework": "5.6.*",
        "rcrowe/twigbridge": "^0.9.6",
        "tymon/jwt-auth": "dev-develop",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~7.0",
        "laravel/tinker": "~1.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "SuperV\\Platform\\PlatformServiceProvider"
            ]
        }
    }
    
}
