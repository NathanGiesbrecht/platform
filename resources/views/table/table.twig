<div class="col-xs-12">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Hover Data Table</h3>
            <div class="pull-right">
                {% for button in buttons %}
                    {{ button.render()|raw }}
                {% endfor %}
            </div>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="row">
                <div class="col-sm-12">
                    <table class="table table-bordered table-striped">

                        {% include 'superv::table/partials/header' %}
                        {% include 'superv::table/partials/body' %}
                    </table>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-5">
                    <div class="dataTables_info" id="example1_info" role="status" aria-live="polite">Showing 51 to 57 of
                        57 entries {{ table.name }}
                    </div>
                </div>
                <div class="col-sm-7">
                    <div class="dataTables_paginate paging_simple_numbers" id="example1_paginate">
                        {% if table.hasPaginator() %}
                            {{ rows.render() | raw }}
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>



