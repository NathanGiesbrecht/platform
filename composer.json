{
    "name": "superv/project",
    "description": "SuperV Project",
    "authors": [
        {
            "name": "Ali Selcuk",
            "email": "maselcuk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "hashids/hashids": "^2.0",
        "kriswallsmith/assetic": "^1.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "rcrowe/twigbridge": "^0.9.5",
        "spatie/laravel-tail": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "~2.3",
        "doctrine/dbal": "^2.6",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "nunomaduro/collision": "^1.1",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "SuperV\\Platform\\": "__workbench__/superv/platform/src/",
            "SuperV\\Droplets\\Supreme\\": "droplets/superv/droplets/supreme/src/",
            "SuperV\\Themes\\Tailwind\\": "droplets/superv/themes/tailwind/src/"
        }
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
            "Tests\\SuperV\\": "tests/",
            "SuperV\\Droplets\\Sample\\": "tests/Platform/__fixtures__/sample-droplet/src/",
            "SuperV\\Themes\\Starter\\": "tests/Platform/__fixtures__/starter-theme/src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "itsgoingd/clockwork"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "repositories": [
    ]
}
